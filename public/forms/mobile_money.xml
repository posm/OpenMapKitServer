          <value>butcher</value>
        </item>
        <item>
          <label>Car</label>
          <value>car</value>
        </item>
        <item>
          <label>Car_parts</label>
          <value>car_parts</value>
        </item>
        <item>
          <label>Car_repair</label>
          <value>car_repair</value>
        </item>
        <item>
          <label>Charcoal</label>
          <value>charcoal</value>
        </item>
        <item>
          <label>Chemist</label>
