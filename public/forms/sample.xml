          <value>no</value>
        </item>
      </select1>
    </group>
    <input ref="/sample_xlsform/select_appearance_note">
      <label>The following questions demo different appearance attributes for select type questions</label>
    </input>
    <group appearance="field-list" ref="/sample_xlsform/labeled_select_group">
      <label>labeled select group test</label>
      <select1 appearance="label" ref="/sample_xlsform/labeled_select_group/label_test">
        <label>label-test</label>
        <item>
          <label ref="jr:itext('/sample_xlsform/labeled_select_group/label_test/yes:label')"/>
