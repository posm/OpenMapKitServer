      <hint>Requires images</hint>
      <item>
        <label ref="jr:itext('/sample_xlsform/compact_test/a:label')"/>
        <value>a</value>
      </item>
      <item>
        <label ref="jr:itext('/sample_xlsform/compact_test/b:label')"/>
        <value>b</value>
      </item>
    </select1>
