        </item>
        <item>
          <label ref="jr:itext('/sample_xlsform/labeled_select_group/label_test/no:label')"/>
          <value>no</value>
        </item>
      </select1>
      <select1 appearance="list-nolabel" ref="/sample_xlsform/labeled_select_group/list-nolabel_test">
        <label>list-nolabel-test</label>
        <item>
          <label ref="jr:itext('/sample_xlsform/labeled_select_group/list-nolabel_test/yes:label')"/>
          <value>yes</value>
        </item>
        <item>
          <label ref="jr:itext('/sample_xlsform/labeled_select_group/list-nolabel_test/no:label')"/>
          <value>no</value>
        </item>
      </select1>
    </group>
    <select1 appearance="compact" ref="/sample_xlsform/compact_test">
